@import './component.styl';
    .point {
        background-color: rgb(0,255,122);
        width: 8px;
        height 8px;
        border-radius 50%;
        box-shadow 0 0 8px 2px grey;
        position absolute;
    }
    .point-one {
        animation pointOne 3s linear infinite normal;
        -webkit-animation: pointOne 3s linear infinite normal;
        left: 31%;
        top: 300px;
    }
    .point-two {
        animation pointTwo 3s linear infinite normal;
        -webkit-animation: pointTwo 3s linear infinite normal;
        left: 47%;
        bottom: 96px;
    }
    .point-three {
        animation pointThree 3s linear infinite normal;
        -webkit-animation: pointThree 3s linear infinite normal;
        left: 67%;
        bottom: 140px;
    }
    .point-four {
        animation pointFour 3s linear infinite normal;
        -webkit-animation: pointFour 3s linear infinite normal;
        left: 67%;
        bottom: 140px;
    }
    .point-five {
        animation pointFive 3s linear infinite normal;
        -webkit-animation: pointFive 3s linear infinite normal;
        top: 309px;
        right: 13%;
    }
    .point-six {
        animation pointSix 3s linear infinite normal;
        -webkit-animation: pointSix 3s linear infinite normal;
        top: 302px;
        right: 38%;    
    }
    @keyframes pointOne {
        0% {
            left: 31%;
            top: 300px;
        }
        25% {
             left: 34%;
             top: 320px;
        }
        70% {
             left: 28%;
             top: 370px;
        }
        100% {
             left: 31%;
             top: 394px
        }
    }
    @-webkit-keyframes pointOne {
        0% {
            left: 31%;
            top: 300px;
        }
        25% {
             left: 34%;
             top: 320px;
        }
        70% {
             left: 28%;
             top: 370px;
        }
        100% {
             left: 31%;
             top: 394px
        }
    }
    @keyframes pointTwo {
        0% {
            left: 47%;
            bottom: 96px;
        }
        50% {
            left: 53%;
            bottom: 42px;
        }
        100% {
            left: 61%;
            bottom: 100px
        }
    }
