@import "./card-content-empty.styl"
.container {
  display: block;
  //max-height 620px;
  overflow-y auto;

  .dic-all-con {
    width: 100%;
  }
  .dic-con {
    /*margin: 1% 0;*/
    h1 {
      padding 5px 10px;
      font-size 16px;
      background: linear-gradient(to right, #c2dcfe, #fff, #fff);

      .da-title {
        color: #333;
        font-weight: blod;
        padding 2px 2px;
        display inline-block;
      }
    }

    .dic-upper {
      display: inline-block;
      margin: 4% 0 0 0;
    }
    .dic-downer {
      display: inline-block;
      margin: 0 0 2% 0;
    }

    .dic-des-con {
      margin: 1% 0;
      overflow: hidden;
      white-space: nowrap;
      text-overflow: ellipsis;
    }

    .dic-row {
      padding 0 10px;
    }
  }
}

.sub-catalog-card .el-card__header {
  background-color: #ebeef5;
  padding: 10px 20px;
}

.data-con {
  width 100%;
  float left;
  padding 10px;
  box-sizing border-box

  h1 {
    font-size 18px;
  }

  .data-item {
    width: 70%;
    display: block;
    float: left;

    li {
      padding 2px 3%;
    }
    li.item-special {
      padding 2% 3%;
    }
  }
  .btn-area {
    width 29%
    float: left;
  }
  .btn {
    margin 0 6%;
  }
}
