.container {
  display: block;
  height: 100%;
  overflow: auto;
  // padding: 0 0 150px 0;
}

.ise-header {
  width 100%;
  height 40px;
  padding: 20px 0;
}

.ise-content {
  .c-title {
    height 35px;
    margin: 0 5px;
    img {
      width: 22px;
      height: 22px;
    }
  }

  .content-bg {
    background: #F8F9F9;
    border 1px solid #EAEAEA;
    border-radius: 4px;
    //margin: 0 5px;
  }
  .cl-content {
    height: 720px;
    overflow-y: auto;
  }
  .customer-tree-node {
    width: 30%;
    float: left;
    i {
      color: rgb(210, 210, 210)
    }
    i.selected {
      color: rgb(252, 198, 50)
    }
  }
  .cm-content {
    margin: 0 5px;
  }
  .btn-br {
    border-radius: 15px;
    padding: 5px 10px;
  }
  .area-top {
    height 320px;
  }
  .area-bottom {
    height: 323px;
  }
  .cm-center-content {
    button {
      margin: 4px 5px;
      padding: 6px 10px;
      width: 75px;
    }
  }
}

.ise-bottom {
  height: 40px;
}

.title-font {
  font-size: 18px;
  padding: 3px;
  display: inline-block;
}

div.cl-content.cm-center-content {
  padding: 10px 20px 30px;
  max-height: 720px;
  overflow-y: auto;
  .dic-all-con {
    width: 100%;
    padding-bottom: 15px;
  }
  .dic-con {
    /*margin: 1% 0;*/
    h1 {
      padding 5px 10px;
      font-size 16px;
      background: linear-gradient(to right, #c2dcfe, #fff, #fff);

      .da-title {
        color: #333;
        font-weight: blod;
        padding 2px 2px;
        display inline-block;
      }
    }

    .dic-des-con {
      margin: 1% 0;
      overflow: hidden;
      white-space: nowrap;
      text-overflow: ellipsis;
    }

    .dic-row {
      padding 0 10px;
    }
  }
}

